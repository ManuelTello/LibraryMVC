<table class="table table-striped-columns my-4">
    <thead class="table-primary">
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Author</th>
            <th>Published</th>
        </tr>
    </thead>
    <tbody>
        <%Payload.forEach(b =>{%>
            <tr>
                <td><%= b._Id %></td>
                <td><%= b._Title %></td>
                <td><%= b._Author %></td>
                <td><%=b._Published.getDate()%>/<%=b._Published.getMonth()%>/<%=b._Published.getFullYear()%></td>
            </tr>
        <%})%>
    </tbody>
</table>
